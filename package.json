{
  "name": "@uiw/react-code-preview",
  "version": "2.1.3",
  "description": "Code edit preview for React.",
  "author": "Kenny Wang <wowohoo@qq.com>",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "scripts": {
    "prepare": "npm run build",
    "released": "npm run build && npm run doc",
    "doc": "kkt build --app-src ./website",
    "start": "kkt start --app-src ./website",
    "watch": "tsbb watch & npm run css:watch",
    "build": "tsbb build && npm run css:build && npm run css:build:dist",
    "test": "tsbb test --env=jsdom",
    "coverage": "tsbb test --env=jsdom --coverage",
    "css:build": "compile-less -d src -o esm",
    "css:watch": "compile-less -d src -o esm --watch",
    "css:build:dist": "compile-less -d src --combine dist.css --rm-global"
  },
  "homepage": "https://uiwjs.github.io/react-code-preview",
  "repository": {
    "type": "git",
    "url": "https://github.com/uiwjs/react-code-preview"
  },
  "files": [
    "src",
    "cjs",
    "esm"
  ],
  "keywords": [
    "uiw",
    "uiwjs",
    "code",
    "code-preview",
    "react-code-preview"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "devDependencies": {
    "@kkt/less-modules": "6.11.0",
    "@kkt/raw-modules": "6.11.0",
    "@kkt/scope-plugin-options": "6.11.0",
    "@types/babel__standalone": "7.1.3",
    "@types/react": "17.0.24",
    "@types/react-dom": "17.0.9",
    "@types/react-test-renderer": "17.0.1",
    "@types/react-transition-group": "4.4.3",
    "@types/webpack-node-externals": "2.5.2",
    "@uiw/react-github-corners": "1.5.3",
    "@uiw/react-markdown-preview": "3.3.3",
    "@uiw/reset.css": "1.0.5",
    "compile-less-cli": "1.8.9",
    "jest-environment-jsdom": "27.2.4",
    "kkt": "6.11.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2",
    "tsbb": "3.3.7",
    "uiw": "4.9.7"
  },
  "dependencies": {
    "@babel/plugin-transform-classes": "7.14.9",
    "@babel/runtime": "7.15.3",
    "@babel/standalone": "7.12.18",
    "@babel/core": "7.15.5",
    "@uiw/copy-to-clipboard": "1.0.12",
    "@uiw/react-codemirror": "4.0.6",
    "@codemirror/lang-javascript": "0.19.2",
    "@uiw/react-split": "5.8.4",
    "@uiw/react-codesandbox": "1.1.0",
    "@uiw/react-codepen": "1.0.2",
    "babel-plugin-transform-remove-imports": "1.7.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
